#include <iostream>
using namespace std;
int main() {
    int n;
    cout<<"Enter the number of elements: ";
    cin>>n;
    int a[n];
    int min = 0;
    for (int i=0; i<n;i++) {
        cin>>a[i];
        if (a[i]<a[i-1]||i==0) {
            min = a[i];
        } {}
    }
    cout<<"The minimum is: "<<min;
    return 0;
}
